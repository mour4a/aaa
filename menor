programa {
  funcao inicio() {
    real a, b, c, menor
    escreva ("digite o primeiro valor: \n")
    leia (a)
    escreva ("digite o segundo valor: \n")
    leia (b)
    escreva ("digite o terceiro valor: \n")
    leia (c)
    se ((a<b)e (a<c))
    menor=a
    senao
    se (b<c)
    menor=b
    senao 
    menor=c
    escreva("MENOR=", menor)
    }
}
